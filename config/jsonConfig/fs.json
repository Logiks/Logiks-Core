        "dir_rules": {
            "/^tmp/": {
                "read": false,
                "write": false,
                "rm": false
            }
        },
	"app":{
            "driver":"local"
        },
        "upload":{
            "driver":"local",
            "basedir":"#APPROOT##APPS_USERDATA_FOLDER#"
        }
    }
}
